{
  "name": "twinte-front",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Twin:te <twinte.net@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "nuxt-ts build",
    "start": "nuxt-ts build && nuxt-ts start",
    "dev": "nuxt-ts",
    "generate": "nuxt-ts generate",
    "format": "prettier --write .",
    "test": "jest"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.5.4",
    "@types/jest": "^24.0.25",
    "@types/lodash": "^4.14.149",
    "husky": "^4.2.3",
    "jest": "^24.9.0",
    "lint-staged": "^10.1.2",
    "node-sass": "^4.13.0",
    "prettier": "^2.0.1",
    "sass-loader": "^8.0.0",
    "ts-jest": "^24.2.0",
    "vue-eslint-parser": "^7.0.0"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.3.5",
    "@nuxtjs/pwa": "^3.0.0-beta.16",
    "@nuxtjs/sentry": "^3.0.1",
    "axios": "^0.19.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.union": "^4.6.0",
    "material-icons": "^0.3.1",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.11.0",
    "nuxt-property-decorator": "^2.3.0",
    "ts-node": "^8.5.4",
    "vue-sweetalert2": "^2.1.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__test__/**/*.test.ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write ."
    ]
  }
}
